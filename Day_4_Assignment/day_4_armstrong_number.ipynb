    "for num in range(1042000,702648266):\n",
    "    order=len(str(num));\n",
    "    add=0;\n",
    "    temp=num;\n",
    "    while temp>0:\n",
    "        digit=temp%10;\n",
    "        add += digit**order;\n",
    "        temp=temp//10;\n",
    "    if(num==add):\n",
    "        print(num);\n",
    "        break;"
   ]
